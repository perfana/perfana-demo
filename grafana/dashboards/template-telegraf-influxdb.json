      "targets": [
        {
          "alias": "Usage",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-telegraf"
          },
          "dsType": "influxdb",
          "groupBy": [
            {
              "params": [
                "$__interval"
              ],
              "type": "time"
            },
            {
              "params": [
                "null"
              ],
              "type": "fill"
            }
          ],
          "measurement": "docker_container_cpu",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT last(\"usage_percent\") FROM \"docker_container_cpu\" WHERE (\"container_name\" =~ /^$container$/ AND \"system_under_test\"::tag =~ /^$system_under_test$/ AND \"test_environment\"::tag =~ /^$test_environment$/) AND $timeFilter GROUP BY time($__interval) fill(null)",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "usage_percent"
                ],
                "type": "field"
              },
              {
                "params": [],
                "type": "last"
              }
            ]
          ],
          "tags": [
            {
              "key": "container_name",
              "operator": "=~",
              "value": "/^$container$/"
            },
            {
              "condition": "AND",
              "key": "system_under_test::tag",
              "operator": "=~",
              "value": "/^$system_under_test$/"
            },
            {
              "condition": "AND",
              "key": "test_environment::tag",
              "operator": "=~",
              "value": "/^$test_environment$/"
            }
          ]
        }
      ],
      "title": "CPU",
      "type": "timeseries"
